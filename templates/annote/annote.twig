<dl class="annote">
	{% for qualifier in qualifiers %}
		{% for touple in qualifier %}
			{% if loop.first %}
				<dt class="annote__verb">
					{% include 'thing' with { globalID: touple.propertyGlobalID } %}
				</dt>
			{% endif %}
			<dd class="annote__object">
				{% include 'snack' with { mainsnak: touple } %} 
			</dd>
		{% endfor  %}
	{% endfor %}
</dl>